{
    "name": "coderomeos/laravel-inertia-react-shadcn-boilerplate",
    "type": "project",
    "description": "The skeleton inertiajs with shadcn boilerplate application template for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework",
        "inertia",
        "shadcn",
        "boilerplate"
    ],
    "authors": [
        {
            "name": "Abid Raza",
            "email": "ari.abid@gmail.com",
            "homepage": "https://github.com/ariabid",
            "role": "Developer"
        },
        {
            "name": "Shubham Surya",
            "email": "shubhamsurya95@gmail.com",
            "homepage": "https://github.com/ninja1ndia",
            "role": "Developer"
        },
        {
            "name": "Gautam Kumar",
            "email": "gtmkmr2u@gmail.com",
            "homepage": "https://github.com/r8here",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-query-builder": "^5.8",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
